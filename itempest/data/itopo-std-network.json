{
  "name": "std-network",
  "networks": [
    {
      "name": "std-network",
      "subnets": [
        {
          "name": "std-subnet",
          "cidr": "192.168.99.0/24",
          "gateway_ip": "192.168.99.253",
          "dns_nameservers": [
            "10.132.71.1",
            "10.132.71.2"
          ],
          "enable_dhcp": true,
          "allocation_pools": [
            {
              "start": "192.168.99.101",
              "end": "192.168.99.199"
            }
          ]
        }
      ]
    }
  ],
  "router-options": {
  },
  "routers": [
    {
      "name": "std-router",
      "gateway": "public",
      "interfaces": ["std-network"]
    }
  ],
  "server-options": {
    "image": "cirros-0.3.3-x86_64-disk",
    "flavor": "m1.tiny",
    "user_data": "./metadata/itempest-userdata"
  },
  "servers": [
    {
      "name": "std-Ka-server",
      "interface": "std-network"
    },
    {
      "name": "std-Sa-server",
      "interface": "std-network"
    }
  ],
  "security-groups": [
    {
      "name": "loginable",
      "rules": [
        "ingress-ssh",
        "ingress-icmp"
      ]
    }
  ],
  "security-group-rules": [
    {
      "name": "ingress-ssh",
      "direction": "ingress",
      "ethertype": "IPv4",
      "protocol": "tcp",
      "port_range_min": 22,
      "port_range_max": 22
    },
    {
      "name": "ingress-icmp",
      "direction": "ingress",
      "ethertype": "IPv4",
      "protocol": "icmp"
    }
  ]
}

